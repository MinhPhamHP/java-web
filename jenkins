pipeline {

    agent any

    tools {
        maven 'my-maven'
    }
    stages {
        stage('Checkout') {
            steps {
                git branch: 'master', 
                    url: 'https://github.com/MinhPhamHP/java-web', 
                    credentialsId: 'Git-credential'
            }
        }
        
        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}

